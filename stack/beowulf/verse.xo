  block
    block |siþðan goldsele|
    block |Grendel warode,|
  block
    block |unriht æfnde,|
    block |oþþæt ende becwom,|
  block
    block |swylt æfter synnum.|
    block |Þæt gesyne wearþ,|
  block
    block |widcuþ werum,|
    block |þætte wrecend þa gyt|
  block
    block |lifde æfter laþum,|
    block |lange þrage,|
  block
    block |æfter guðceare.|
    block |Grendles modor,|
  block
    block |ides, aglæcwif,|
    block |yrmþe gemunde,|
  block
    block |se þe wæteregesan|
    block |wunian scolde,|
  block
    block |cealde streamas,|
    block |siþðan Cain wearð|
  block
    block |to ecgbanan|
    block |angan breþer,|
  block
    block |fæderenmæge;|
    block |he þa fag gewat,|
  block
    block |morþre gemearcod,|
    block |mandream fleon,|
  block
    block |westen warode.|
    block |Þanon woc fela|
  block
    block |geosceaftgasta;|
    block |wæs þæra Grendel sum,|
  block
    block |heorowearh hetelic,|
    block |se æt Heorote fand|
  block
    block |wæccendne wer|
    block |wiges bidan.|
  block
    block |Þær him aglæca|
    block |ætgræpe wearð;|
  block
    block |hwæþre he gemunde|
    block |mægenes strenge,|
  block
    block |gimfæste gife|
    block |ðe him god sealde,|
  block
    block |ond him to anwaldan|
    block |are gelyfde,|
  block
    block |frofre ond fultum;|
    block |ðy he þone feond ofercwom,|
  block
    block |gehnægde helle gast.|
    block |Þa he hean gewat,|
  block
    block |dreame bedæled,|
    block |deaþwic seon,|
  block
    block |mancynnes feond,|
    block |ond his modor þa gyt,|
  block
    block |gifre ond galgmod,|
    block |gegan wolde|
  block
    block |sorhfulne sið,|
    block |sunu deað wrecan.|
  block
    block |Com þa to Heorote,|
    block |ðær Hringdene|
  block
    block |geond þæt sæld swæfun.|
    block |Þa ðær sona wearð|
  block
    block |edhwyrft eorlum,|
    block |siþðan inne fealh|
  block
    block |Grendles modor.|
    block |Wæs se gryre læssa|
  block
    block |efne swa micle|
    block |swa bið mægþa cræft,|
  block
    block |wiggryre wifes,|
    block |be wæpnedmen,|
  block
    block |þonne heoru bunden,|
    block |hamere geþuren,|
  block
    block |sweord swate fah|
    block |swin ofer helme|
  block
    block |ecgum dyhttig|
    block |andweard scireð.|
  block
    block |Þa wæs on healle|
    block |heardecg togen|
  block
    block |sweord ofer setlum,|
    block |sidrand manig|
  block
    block |hafen handa fæst;|
    block |helm ne gemunde,|
  block
    block |byrnan side,|
    block |þa hine se broga angeat.|
  block
    block |Heo wæs on ofste,|
    block |wolde ut þanon,|
  block
    block |feore beorgan,|
    block |þa heo onfunden wæs.|
  block
    block |Hraðe heo æþelinga|
    block |anne hæfde|
  block
    block |fæste befangen,|
    block |þa heo to fenne gang.|
  block
    block |Se wæs Hroþgare|
    block |hæleþa leofost|
  block
    block |on gesiðes had|
    block |be sæm tweonum,|
  block
    block |rice randwiga,|
    block |þone ðe heo on ræste abreat,|
  block
    block |blædfæstne beorn.|
    block |Næs Beowulf ðær,|
  block
    block |ac wæs oþer in|
    block |ær geteohhod|
  block
    block |æfter maþðumgife|
    block |mærum Geate.|
  block
    block |Hream wearð in Heorote;|
    block |heo under heolfre genam|
  block
    block |cuþe folme;|
    block |cearu wæs geniwod,|
  block
    block |geworden in wicun.|
    block |Ne wæs þæt gewrixle til,|
  block
    block |þæt hie on ba healfa|
    block |bicgan scoldon|
  block
    block |freonda feorum.|
    block |Þa wæs frod cyning,|
  block
    block |har hilderinc,|
    block |on hreon mode,|
  block
    block |syðþan he aldorþegn|
    block |unlyfigendne,|
  block
    block |þone deorestan|
    block |deadne wisse.|
  block
    block |Hraþe wæs to bure|
    block |Beowulf fetod,|
  block
    block |sigoreadig secg.|
    block |Samod ærdæge|
  block
    block |eode eorla sum,|
    block |æþele cempa|
  block
    block |self mid gesiðum|
    block |þær se snotera bad,|
  block
    block |hwæþer him alwalda|
    block |æfre wille|
  block
    block |æfter weaspelle|
    block |wyrpe gefremman.|
  block
    block |Gang ða æfter flore|
    block |fyrdwyrðe man|
  block
    block |mid his handscale|
    block |(healwudu dynede),|
  block
    block |þæt he þone wisan|
    block |wordum nægde|
  block
    block |frean Ingwina,|
    block |frægn gif him wære|
  block
    block |æfter neodlaðum|
    block |niht getæse.|

  block
    block |Hroðgar maþelode,|
    block |helm Scyldinga:|
  block
    block |"Ne frin þu æfter sælum!|
    block |Sorh is geniwod|
  block
    block |Denigea leodum.|
    block |Dead is Æschere,|
  block
    block |Yrmenlafes|
    block |yldra broþor,|
  block
    block |min runwita|
    block |ond min rædbora,|
  block
    block |eaxlgestealla,|
    block |ðonne we on orlege|
  block
    block |hafelan weredon,|
    block |þonne hniton feþan,|
  block
    block |eoferas cnysedan.|
    block |Swylc scolde eorl wesan,|
  block
    block |æþeling ærgod,|
    block |swylc Æschere wæs!|
  block
    block |Wearð him on Heorote|
    block |to handbanan|
  block
    block |wælgæst wæfre;|
    block |ic ne wat hwæder|
  block
    block |atol æse wlanc|
    block |eftsiðas teah,|
  block
    block |fylle gefægnod.|
    block |Heo þa fæhðe wræc|
  block
    block |þe þu gystran niht|
    block |Grendel cwealdest|
  block
    block |þurh hæstne had|
    block |heardum clammum,|
  block
    block |forþan he to lange|
    block |leode mine|
  block
    block |wanode ond wyrde.|
    block |He æt wige gecrang|
  block
    block |ealdres scyldig,|
    block |ond nu oþer cwom|
  block
    block |mihtig manscaða,|
    block |wolde hyre mæg wrecan,|
  block
    block |ge feor hafað|
    block |fæhðe gestæled|
  block
    block |(þæs þe þincean mæg|
    block |þegne monegum,|
  block
    block |se þe æfter sincgyfan|
    block |on sefan greoteþ),|
  block
    block |hreþerbealo hearde;|
    block |nu seo hand ligeð,|
  block
    block |se þe eow welhwylcra|
    block |wilna dohte.|
  block
    block |Ic þæt londbuend,|
    block |leode mine,|
  block
    block |selerædende,|
    block |secgan hyrde|
  block
    block |þæt hie gesawon|
    block |swylce twegen|
  block
    block |micle mearcstapan|
    block |moras healdan,|
  block
    block |ellorgæstas.|
    block |Ðæra oðer wæs,|
  block
    block |þæs þe hie gewislicost|
    block |gewitan meahton,|
  block
    block |idese onlicnæs;|
    block |oðer earmsceapen|
  block
    block |on weres wæstmum|
    block |wræclastas træd,|
  block
    block |næfne he wæs mara|
    block |þonne ænig man oðer;|
  block
    block |þone on geardagum|
    block |Grendel nemdon|
  block
    block |foldbuende.|
    block |No hie fæder cunnon,|
  block
    block |hwæþer him ænig wæs|
    block |ær acenned|
  block
    block |dyrnra gasta.|
    block |Hie dygel lond|
  block
    block |warigeað, wulfhleoþu,|
    block |windige næssas,|
  block
    block |frecne fengelad,|
    block |ðær fyrgenstream|
  block
    block |under næssa genipu|
    block |niþer gewiteð,|
  block
    block |flod under foldan.|
    block |Nis þæt feor heonon|
  block
    block |milgemearces|
    block |þæt se mere standeð;|
  block
    block |ofer þæm hongiað|
    block |hrinde bearwas,|
  block
    block |wudu wyrtum fæst|
    block |wæter oferhelmað.|
  block
    block |Þær mæg nihta gehwæm|
    block |niðwundor seon,|
  block
    block |fyr on flode.|
    block |No þæs frod leofað|
  block
    block |gumena bearna,|
    block |þæt þone grund wite;|
  block
    block |ðeah þe hæðstapa|
    block |hundum geswenced,|
  block
    block |heorot hornum trum,|
    block |holtwudu sece,|
  block
    block |feorran geflymed,|
    block |ær he feorh seleð,|
  block
    block |aldor on ofre,|
    block |ær he in wille|
  block
    block |hafelan hydan.|
    block |Nis þæt heoru stow!|
  block
    block |Þonon yðgeblond|
    block |up astigeð|
  block
    block |won to wolcnum,|
    block |þonne wind styreþ,|
  block
    block |lað gewidru,|
    block |oðþæt lyft drysmaþ,|
  block
    block |roderas reotað.|
    block |Nu is se ræd gelang|
  block
    block |eft æt þe anum.|
    block |Eard git ne const,|
  block
    block |frecne stowe,|
    block |ðær þu findan miht|
  block
    block |felasinnigne secg;|
    block |sec gif þu dyrre.|
  block
    block |Ic þe þa fæhðe|
    block |feo leanige,|
  block
    block |ealdgestreonum,|
    block |swa ic ær dyde,|
  block
    block |wundnum golde,|
    block |gyf þu on weg cymest."|

  block
    block |Beowulf maþelode,|
    block |bearn Ecgþeowes:|
  block
    block |"Ne sorga, snotor guma;|
    block |selre bið æghwæm|
  block
    block |þæt he his freond wrece,|
    block |þonne he fela murne.|
  block
    block |Ure æghwylc sceal|
    block |ende gebidan|
  block
    block |worolde lifes;|
    block |wyrce se þe mote|
  block
    block |domes ær deaþe;|
    block |þæt bið drihtguman|
  block
    block |unlifgendum|
    block |æfter selest.|
  block
    block |Aris, rices weard,|
    block |uton raþe feran|
  block
    block |Grendles magan|
    block |gang sceawigan.|
  block
    block |Ic hit þe gehate,|
    block |no he on helm losaþ,|
  block
    block |ne on foldan fæþm,|
    block |ne on fyrgenholt,|
  block
    block |ne on gyfenes grund,|
    block |ga þær he wille.|
  block
    block |Ðys dogor þu|
    block |geþyld hafa|
  block
    block |weana gehwylces,|
    block |swa ic þe wene to."|
  block
    block |Ahleop ða se gomela,|
    block |gode þancode,|
  block
    block |mihtigan drihtne,|
    block |þæs se man gespræc.|
  block
    block |Þa wæs Hroðgare|
    block |hors gebæted,|
  block
    block |wicg wundenfeax.|
    block |Wisa fengel|
  block
    block |geatolic gende;|
    block |gumfeþa stop|
  block
    block |lindhæbbendra.|
    block |Lastas wæron|
  block
    block |æfter waldswaþum|
    block |wide gesyne,|
  block
    block |gang ofer grundas,|
    block |þær heo gegnum for|
  block
    block |ofer myrcan mor,|
    block |magoþegna bær|
  block
    block |þone selestan|
    block |sawolleasne|
  block
    block |þara þe mid Hroðgare|
    block |ham eahtode.|
  block
    block |Ofereode þa|
    block |æþelinga bearn|
  block
    block |steap stanhliðo,|
    block |stige nearwe,|
  block
    block |enge anpaðas,|
    block |uncuð gelad,|
  block
    block |neowle næssas,|
    block |nicorhusa fela.|
  block
    block |He feara sum|
    block |beforan gengde|
  block
    block |wisra monna|
    block |wong sceawian,|
  block
    block |oþþæt he færinga|
    block |fyrgenbeamas|
  block
    block |ofer harne stan|
    block |hleonian funde,|
  block
    block |wynleasne wudu;|
    block |wæter under stod|
  block
    block |dreorig ond gedrefed.|
    block |Denum eallum wæs,|
  block
    block |winum Scyldinga,|
    block |weorce on mode|
  block
    block |to geþolianne,|
    block |ðegne monegum,|
  block
    block |oncyð eorla gehwæm,|
    block |syðþan Æscheres|
  block
    block |on þam holmclife|
    block |hafelan metton.|
  block
    block |Flod blode weol|
    block |(folc to sægon),|
  block
    block |hatan heolfre.|
    block |Horn stundum song|
  block
    block |fuslic fyrdleoð.|
    block |Feþa eal gesæt.|
  block
    block |Gesawon ða æfter wætere|
    block |wyrmcynnes fela,|
  block
    block |sellice sædracan,|
    block |sund cunnian,|
  block
    block |swylce on næshleoðum|
    block |nicras licgean,|
  block
    block |ða on undernmæl|
    block |oft bewitigað|
  block
    block |sorhfulne sið|
    block |on seglrade,|
  block
    block |wyrmas ond wildeor;|
    block |hie on weg hruron,|
  block
    block |bitere ond gebolgne,|
    block |bearhtm ongeaton,|
  block
    block |guðhorn galan.|
    block |Sumne Geata leod|
  block
    block |of flanbogan|
    block |feores getwæfde,|
  block
    block |yðgewinnes,|
    block |þæt him on aldre stod|
  block
    block |herestræl hearda;|
    block |he on holme wæs|
  block
    block |sundes þe sænra,|
    block |ðe hyne swylt fornam.|
  block
    block |Hræþe wearð on yðum|
    block |mid eoferspreotum|
  block
    block |heorohocyhtum|
    block |hearde genearwod,|
  block
    block |niða genæged,|
    block |ond on næs togen,|
  block
    block |wundorlic wægbora;|
    block |weras sceawedon|
  block
    block |gryrelicne gist.|
    block |Gyrede hine Beowulf|
  block
    block |eorlgewædum,|
    block |nalles for ealdre mearn.|
  block
    block |Scolde herebyrne|
    block |hondum gebroden,|
  block
    block |sid ond searofah,|
    block |sund cunnian,|
  block
    block |seo ðe bancofan|
    block |beorgan cuþe,|
  block
    block |þæt him hildegrap|
    block |hreþre ne mihte,|
  block
    block |eorres inwitfeng,|
    block |aldre gesceþðan;|
  block
    block |ac se hwita helm|
    block |hafelan werede,|
  block
    block |se þe meregrundas|
    block |mengan scolde,|
  block
    block |secan sundgebland|
    block |since geweorðad,|
  block
    block |befongen freawrasnum,|
    block |swa hine fyrndagum|
  block
    block |worhte wæpna smið,|
    block |wundrum teode,|
  block
    block |besette swinlicum,|
    block |þæt hine syðþan no|
  block
    block |brond ne beadomecas|
    block |bitan ne meahton.|
  block
    block |Næs þæt þonne mætost|
    block |mægenfultuma|
  block
    block |þæt him on ðearfe lah|
    block |ðyle Hroðgares;|
  block
    block |wæs þæm hæftmece|
    block |Hrunting nama.|
  block
    block |Þæt wæs an foran|
    block |ealdgestreona;|
  block
    block |ecg wæs iren,|
    block |atertanum fah,|
  block
    block |ahyrded heaþoswate;|
    block |næfre hit æt hilde ne swac|
  block
    block |manna ængum|
    block |þara þe hit mid mundum bewand,|
  block
    block |se ðe gryresiðas|
    block |gegan dorste,|
  block
    block |folcstede fara;|
    block |næs þæt forma sið|
  block
    block |þæt hit ellenweorc|
    block |æfnan scolde.|
  block
    block |Huru ne gemunde|
    block |mago Ecglafes,|
  block
    block |eafoþes cræftig,|
    block |þæt he ær gespræc|
  block
    block |wine druncen,|
    block |þa he þæs wæpnes onlah|
  block
    block |selran sweordfrecan.|
    block |Selfa ne dorste|
  block
    block |under yða gewin|
    block |aldre geneþan,|
  block
    block |drihtscype dreogan;|
    block |þær he dome forleas,|
  block
    block |ellenmærðum.|
    block |Ne wæs þæm oðrum swa,|
  block
    block |syðþan he hine to guðe|
    block |gegyred hæfde.|

  block
    block |Beowulf maðelode,|
    block |bearn Ecgþeowes:|
  block
    block |"Geþenc nu, se mæra|
    block |maga Healfdenes,|
  block
    block |snottra fengel,|
    block |nu ic eom siðes fus,|
  block
    block |goldwine gumena,|
    block |hwæt wit geo spræcon,|
  block
    block |gif ic æt þearfe|
    block |þinre scolde|
  block
    block |aldre linnan,|
    block |þæt ðu me a wære|
  block
    block |forðgewitenum|
    block |on fæder stæle.|
  block
    block |Wes þu mundbora|
    block |minum magoþegnum,|
  block
    block |hondgesellum,|
    block |gif mec hild nime;|
  block
    block |swylce þu ða madmas|
    block |þe þu me sealdest,|
  block
    block |Hroðgar leofa,|
    block |Higelace onsend.|
  block
    block |Mæg þonne on þæm golde ongitan|
    block |Geata dryhten,|
  block
    block |geseon sunu Hrædles,|
    block |þonne he on þæt sinc starað,|
  block
    block |þæt ic gumcystum|
    block |godne funde|
  block
    block |beaga bryttan,|
    block |breac þonne moste.|
  block
    block |Ond þu Unferð læt|
    block |ealde lafe,|
  block
    block |wrætlic wægsweord,|
    block |widcuðne man|
  block
    block |heardecg habban;|
    block |ic me mid Hruntinge|
  block
    block |dom gewyrce,|
    block |oþðe mec deað nimeð."|
  block
    block |æfter þæm wordum|
    block |Wedergeata leod|
  block
    block |efste mid elne,|
    block |nalas ondsware|
  block
    block |bidan wolde;|
    block |brimwylm onfeng|
  block
    block |hilderince.|
    block |Ða wæs hwil dæges|
  block
    block |ær he þone grundwong|
    block |ongytan mehte.|
  block
    block |Sona þæt onfunde|
    block |se ðe floda begong|
  block
    block |heorogifre beheold|
    block |hund missera,|
  block
    block |grim ond grædig,|
    block |þæt þær gumena sum|
  block
    block |ælwihta eard|
    block |ufan cunnode.|
  block
    block |Grap þa togeanes,|
    block |guðrinc gefeng|
  block
    block |atolan clommum.|
    block |No þy ær in gescod|
  block
    block |halan lice;|
    block |hring utan ymbbearh,|
  block
    block |þæt heo þone fyrdhom|
    block |ðurhfon ne mihte,|
  block
    block |locene leoðosyrcan|
    block |laþan fingrum.|
  block
    block |Bær þa seo brimwylf,|
    block |þa heo to botme com,|
  block
    block |hringa þengel|
    block |to hofe sinum,|
  block
    block |swa he ne mihte,|
    block |no he þæs modig wæs,|
  block
    block |wæpna gewealdan,|
    block |ac hine wundra þæs fela|
  block
    block |swencte on sunde,|
    block |sædeor monig|
  block
    block |hildetuxum|
    block |heresyrcan bræc,|
  block
    block |ehton aglæcan.|
    block |Ða se eorl ongeat|
  block
    block |þæt he in niðsele|
    block |nathwylcum wæs,|
  block
    block |þær him nænig wæter|
    block |wihte ne sceþede,|
  block
    block |ne him for hrofsele|
    block |hrinan ne mehte|
  block
    block |færgripe flodes;|
    block |fyrleoht geseah,|
  block
    block |blacne leoman,|
    block |beorhte scinan.|
  block
    block |Ongeat þa se goda|
    block |grundwyrgenne,|
  block
    block |merewif mihtig;|
    block |mægenræs forgeaf|
  block
    block |hildebille,|
    block |hond sweng ne ofteah,|
  block
    block |þæt hire on hafelan|
    block |hringmæl agol|
  block
    block |grædig guðleoð.|
    block |Ða se gist onfand|
  block
    block |þæt se beadoleoma|
    block |bitan nolde,|
  block
    block |aldre sceþðan,|
    block |ac seo ecg geswac|
  block
    block |ðeodne æt þearfe;|
    block |ðolode ær fela|
  block
    block |hondgemota,|
    block |helm oft gescær,|
  block
    block |fæges fyrdhrægl;|
    block |ða wæs forma sið|
  block
    block |deorum madme,|
    block |þæt his dom alæg.|
  block
    block |Eft wæs anræd,|
    block |nalas elnes læt,|
  block
    block |mærða gemyndig|
    block |mæg Hylaces.|
  block
    block |Wearp ða wundenmæl|
    block |wrættum gebunden|
  block
    block |yrre oretta,|
    block |þæt hit on eorðan læg,|
  block
    block |stið ond stylecg;|
    block |strenge getruwode,|
  block
    block |mundgripe mægenes.|
    block |Swa sceal man don,|
  block
    block |þonne he æt guðe|
    block |gegan þenceð|
  block
    block |longsumne lof,|
    block |na ymb his lif cearað.|
  block
    block |Gefeng þa be eaxle|
    block |(nalas for fæhðe mearn)|
  block
    block |Guðgeata leod|
    block |Grendles modor;|
  block
    block |brægd þa beadwe heard,|
    block |þa he gebolgen wæs,|
  block
    block |feorhgeniðlan,|
    block |þæt heo on flet gebeah.|
  block
    block |Heo him eft hraþe|
    block |andlean forgeald|
  block
    block |grimman grapum|
    block |ond him togeanes feng;|
  block
    block |oferwearp þa werigmod|
    block |wigena strengest,|
  block
    block |feþecempa,|
    block |þæt he on fylle wearð.|
  block
    block |Ofsæt þa þone selegyst|
    block |ond hyre seax geteah,|
  block
    block |brad ond brunecg,|
    block |wolde hire bearn wrecan,|
  block
    block |angan eaferan.|
    block |Him on eaxle læg|
  block
    block |breostnet broden;|
    block |þæt gebearh feore,|
  block
    block |wið ord ond wið ecge|
    block |ingang forstod.|
  block
    block |Hæfde ða forsiðod|
    block |sunu Ecgþeowes|
  block
    block |under gynne grund,|
    block |Geata cempa,|
  block
    block |nemne him heaðobyrne|
    block |helpe gefremede,|
  block
    block |herenet hearde,|
    block |ond halig god|
  block
    block |geweold wigsigor;|
    block |witig drihten,|
  block
    block |rodera rædend,|
    block |hit on ryht gesced|
  block
    block |yðelice,|
    block |syþðan he eft astod.|

  block
    block |Geseah ða on searwum|
    block |sigeeadig bil,|
  block
    block |eald sweord eotenisc,|
    block |ecgum þyhtig,|
  block
    block |wigena weorðmynd;|
    block |þæt wæs wæpna cyst,|
  block
    block |buton hit wæs mare|
    block |ðonne ænig mon oðer|
  block
    block |to beadulace|
    block |ætberan meahte,|
  block
    block |god ond geatolic,|
    block |giganta geweorc.|
  block
    block |He gefeng þa fetelhilt,|
    block |freca Scyldinga|
  block
    block |hreoh ond heorogrim|
    block |hringmæl gebrægd,|
  block
    block |aldres orwena,|
    block |yrringa sloh,|
  block
    block |þæt hire wið halse|
    block |heard grapode,|
  block
    block |banhringas bræc.|
    block |Bil eal ðurhwod|
  block
    block |fægne flæschoman;|
    block |heo on flet gecrong.|
  block
    block |Sweord wæs swatig,|
    block |secg weorce gefeh.|
  block
    block |Lixte se leoma,|
